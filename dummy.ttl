<https://mock.ld.admin.ch/cube/mock-example/1/shape> a cube:Constraint ;
    sh:closed true ;
    sh:property [ a cube:MeasureDimension ;
            qudt:hasUnit unit:KiloGM ;
            qudt:scaleType qudt:IntervalScale ;
            schema1:description "Gemessener Wert an der Station"@de ;
            schema1:name "Wert"@de,
                "Value"@en,
                "Valeur"@fr,
                "Valore"@it ;
            sh:max 2.355574e+01 ;
            sh:maxCount 1 ;
            sh:min 2.068821e+01 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:path mock:value ],
        [ a cube:KeyDimension ;
            qudt:scaleType qudt:NominalScale ;
            schema1:description "Station der Untersuchung"@de ;
            schema1:name "Station"@de,
                "Station"@en,
                "Station"@fr,
                "Stazione"@it ;
            sh:in ( <https://mock.ld.admin.ch/station/01> <https://mock.ld.admin.ch/station/02> ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:path mock:station ],
        [ a cube:KeyDimension ;
            qudt:scaleType qudt:OrdinalScale ;
            schema1:description "Jahr der Erhebung"@de ;
            schema1:name "Jahr"@de,
                "Year"@en,
                "An"@fr,
                "Anno"@it ;
            sh:in ( <https://ld.admin.ch/time/year/2000> <https://ld.admin.ch/time/year/2001> <https://ld.admin.ch/time/year/2002> ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:path mock:year ;
            meta:dataKind [ a time:GeneralDateTimeDescription ;
                    time:unitType time:unitYear ] ],
        [ qudt:hasUnit unit:PERCENT ;
            qudt:scaleType qudt:RatioScale ;
            schema1:description "Upper Unsicherheit"@de ;
            schema1:name "Upper Unsicherheit"@de ;
            sh:max 1.2e+01 ;
            sh:maxCount 1 ;
            sh:min 4e+00 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:path mock:upperUncertainty ;
            meta:dimensionRelation [ a relation:ConfidenceUpperBound ;
                    dct:type "Upper uncertainty" ;
                    meta:relatesTo mock:value ] ],
        [ qudt:hasUnit unit:PERCENT ;
            qudt:scaleType qudt:RatioScale ;
            schema1:description "Lower Unsicherheit"@de ;
            schema1:name "Lower Unsicherheit"@de ;
            sh:max 4.3e+01 ;
            sh:maxCount 1 ;
            sh:min 1e+00 ;
            sh:minCount 1 ;
            sh:path mock:lowerUncertainty ] .

