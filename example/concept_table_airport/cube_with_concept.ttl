<https://mock-concept.ld.admin.ch/cube/mock-concept/1/shape> a cube:Constraint ;
    sh:closed true ;
    sh:property [ a cube:KeyDimension ;
            qudt:scaleType qudt:NominalScale ;
            schema1:description "Flughafentyp - DESC"@de,
                "Type of Airport - DESC"@en,
                "Type d'aéroport - DESC"@fr,
                "Tipo di aeroporto - DESC"@it ;
            schema1:name "Flughafentyp"@de,
                "Type of Airport"@en,
                "Type d'aéroport"@fr,
                "Tipo di aeroporto"@it ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:path mock:airport_type ],
        [ a cube:KeyDimension ;
            qudt:scaleType qudt:OrdinalScale ;
            schema1:description "Jahr der Erhebung"@de,
                "Year of survey"@en,
                "Année du relevé"@fr,
                "Anno di rilevamento"@it ;
            schema1:name "Jahr"@de,
                "Year"@en,
                "Année"@fr,
                "Anno"@it ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:path mock:Jahr ;
            meta:dataKind [ a time:GeneralDateTimeDescription ;
                    time:unitType time:unitYear ] ],
        [ qudt:scaleType qudt:NominalScale ;
            schema1:description "Flughafentyp - second key for demo"@de,
                "Type of Airport - second key for demo"@en,
                "Type d'aéroport - second key for demo"@fr,
                "Tipo di aeroporto - second key for demo"@it ;
            schema1:name "Flughafentyp (second key for demo)"@de,
                "Type of Airport (second key for demo)"@en,
                "Type d'aéroport (second key for demo)"@fr,
                "Tipo di aeroporto (second key for demo)"@it ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:path mock:airport_type_2nd ],
        [ a cube:MeasureDimension ;
            qudt:hasUnit unit:KiloGM ;
            qudt:scaleType qudt:IntervalScale ;
            schema1:description "Wert - DESC"@de,
                "Value - DESC"@en,
                "Valeur - DESC"@fr,
                "Valore - DESC"@it ;
            schema1:name "Wert"@de,
                "Value"@en,
                "Valeur"@fr,
                "Valore"@it ;
            sh:max "20" ;
            sh:maxCount 1 ;
            sh:min "12" ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:path mock:value ] .

