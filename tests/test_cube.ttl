<https://mock.ld.admin.ch/cube/mock-two-sided/1/shape> a cube:Constraint ;
    sh:closed true ;
    sh:property [ qudt:hasUnit unit:PERCENT ;
            qudt:scaleType qudt:RatioScale ;
            schema1:description "Standardfehler der Schätzung Wert2"@de ;
            schema1:name "Standardfehler für Wert2"@de,
                "standard error for Value2"@en,
                "erreur standard pour Value2"@fr,
                "errore"@it ;
            sh:max "5" ;
            sh:maxCount 1 ;
            sh:min "5" ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:path mock:standardError ;
            meta:dimensionRelation [ a relation:StandardError ;
                    meta:relatesTo mock:value2 ] ],
        [ qudt:scaleType qudt:NominalScale ;
            schema1:description "Status der Veröffentlichung, provisorisch oder final"@de ;
            schema1:name "Veröffentlichungsstatus"@de,
                "State of publication"@en,
                "Statut de publication"@fr,
                "Stato di pubblicazione"@it ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:path mock:status ],
        [ qudt:hasUnit unit:PERCENT ;
            qudt:scaleType qudt:RatioScale ;
            schema1:description "Upper Unsicherheit"@de ;
            schema1:name "Upper Unsicherheit"@de ;
            sh:max 1e+01 ;
            sh:maxCount 1 ;
            sh:min 1e+01 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:path mock:upperUncertainty ;
            meta:dimensionRelation [ a relation:ConfidenceUpperBound ;
                    dct:type "Confidence interval" ;
                    meta:relatesTo mock:value ] ],
        [ a cube:MeasureDimension ;
            qudt:hasUnit unit:KiloGM ;
            qudt:scaleType qudt:IntervalScale ;
            schema1:description "Gemessener Wert an der Station"@de ;
            schema1:name "Wert"@de,
                "Value"@en,
                "Valeur"@fr,
                "Valore"@it ;
            sh:max 2.355574e+01 ;
            sh:maxCount 1 ;
            sh:min 2.068821e+01 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:path mock:value ],
        [ a cube:KeyDimension ;
            qudt:scaleType qudt:OrdinalScale ;
            schema1:description "Jahr der Erhebung"@de ;
            schema1:name "Jahr"@de,
                "Year"@en,
                "An"@fr,
                "Anno"@it ;
            sh:in ( <https://ld.admin.ch/time/year/2000> <https://ld.admin.ch/time/year/2001> <https://ld.admin.ch/time/year/2002> ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:path mock:year ;
            meta:dataKind [ a time:GeneralDateTimeDescription ;
                    time:unitType time:unitYear ] ],
        [ qudt:hasUnit unit:PERCENT ;
            qudt:scaleType qudt:RatioScale ;
            schema1:description "Lower Unsicherheit"@de ;
            schema1:name "Lower Unsicherheit"@de ;
            sh:max 2e+00 ;
            sh:maxCount 1 ;
            sh:min 1e+00 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:path mock:lowerUncertainty ;
            meta:dimensionRelation [ a relation:ConfidenceLowerBound ;
                    dct:type "Confidence interval" ;
                    meta:relatesTo mock:value ] ],
        [ a cube:KeyDimension ;
            qudt:scaleType qudt:NominalScale ;
            schema1:description "Station der Untersuchung"@de ;
            schema1:name "Station"@de,
                "Station"@en,
                "Station"@fr,
                "Stazione"@it ;
            sh:in ( <https://mock.ld.admin.ch/station/01> <https://mock.ld.admin.ch/station/02> ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:path mock:station ],
        [ a cube:MeasureDimension ;
            qudt:hasUnit unit:KiloGM ;
            qudt:scaleType qudt:RatioScale ;
            schema1:description "Gemessener Wert 2 an der Station"@de ;
            schema1:name "Wert2"@de,
                "Value2"@en,
                "Valeur2"@fr,
                "Valore2"@it ;
            sh:max 1.16e+01 ;
            sh:maxCount 1 ;
            sh:min 1.03e+01 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:path mock:value2 ;
            meta:annotation [ a meta:Limit ;
                    schema1:name "Richtwert 2020 in Zürich"@de ;
                    schema1:value 11 ;
                    meta:annotationContext [ sh:hasValue <https://mock.ld.admin.ch/station/02> ;
                            sh:path mock:station ],
                        [ sh:hasValue <https://ld.admin.ch/time/year/2020> ;
                            sh:path mock:year ] ],
                [ a meta:Limit ;
                    schema1:maxValue 13 ;
                    schema1:minValue 9 ;
                    schema1:name "Zielwert für 2021 in Zürich"@de ;
                    meta:annotationContext [ sh:hasValue <https://mock.ld.admin.ch/station/02> ;
                            sh:path mock:station ],
                        [ sh:hasValue <https://ld.admin.ch/time/year/2021> ;
                            sh:path mock:year ] ] ] .

